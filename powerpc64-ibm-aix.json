{
  "arch": "powerpc64",
  "cpu": "ppc64",
  "crt-static-allows-dylibs": true,
  "crt-static-default": true,
  "crt-static-respected": true,
  "data-layout": "E-m:a-i64:64-n32:64",
  "dynamic-linking": true,
  "env": "",
  "executables": true,
  "has-elf-tls": true,
  "has-rpath": false,
  "linker": "xlclang",
  "linker-flavor": "gcc",
  "linker-is-gnu": false,
  "llvm-target": "powerpc64-ibm-aix",
  "max-atomic-width": 64,
  "os": "aix",
  "target-c-int-width": "32",
  "target-endian": "big",
  "target-family": "unix",
  "target-pointer-width": "64",
  "vendor": "ibm",
  "emit-debug-gdb-scripts-comment": "XCOFF explicit sections not yet implemented",
  "emit-debug-gdb-scripts": false,
  "features-comment": "Altivec support is unimplemented on AIX",
  "features": "-altivec",
  "function-sections-comment": "XCOFF unique data sections not yet implemented (current rustc sets fdata-sections=ffunction-sections)",
  "function-sections": false
}
